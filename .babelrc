{
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "assets": "./src/assets",
                    "components": "./src/components",
                    "containers": "./src/containers",
                    "hooks": "./src/hooks",
                    "navigation": "./src/navigation",
                    "screens": "./src/screens",
                    "services": "./src/services",
                    "theme": "./src/theme",
                    "types": "./src/types",
                    "utils": "./src/utils"
                }
            }
        ]
    ]
}